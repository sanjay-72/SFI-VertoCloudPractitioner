    <style>
        .hidden {
            opacity: 0;
            filter: blur(5px);
            transform: translateX(-100%);
            transition: all 0.5s;
        }

        .hiddenR2L {
            opacity: 0;
            filter: blur(5px);
            transform: translateX(+100%);
            transition: all 0.5s;
        }

        .show {
            opacity: 1;
            filter: blur(0);
            transform: translateX(0);
            transition: all 0.5s;
        }


        .hidden:nth-child(2) {
            transition-delay: 100ms;
        }

        .hidden:nth-child(3) {
            transition-delay: 200ms;
        }
